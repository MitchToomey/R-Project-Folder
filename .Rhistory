mutate(ses = factor(ses, levels = c(1, 2, 3), labels = c("low", "middle", "high"))) %>%
ggplot(aes(x = ses, y = math, group = ses, fill = ses)) +
geom_boxplot(alpha = .7) +
geom_jitter(width = .05, alpha = .4) +
guides(fill = "none") +
theme_bw() +
labs(
x = "ses",
y = "math scores"
)
